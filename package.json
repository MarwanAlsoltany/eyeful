{
  "name": "eyeful",
  "version": "1.0.0",
  "description": "A time tracking application with some neat features.",
  "main": "./assets/js/dist/app.bundle.js",
  "scripts": {
    "mix-config": "cp -r node_modules/laravel-mix/setup/** ./",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarwanAlsoltany/eyeful.git"
  },
  "keywords": [
    "HTML",
    "CSS",
    "JavaScript",
    "SWA",
    "PWA",
    "JS",
    "App",
    "Application",
    "Timer",
    "Time",
    "Tracker",
    "Todos",
    "Todo",
    "List"
  ],
  "author": "Marwan Al-Soltany",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarwanAlsoltany/eyeful/issues"
  },
  "homepage": "https://github.com/MarwanAlsoltany/eyeful#readme",
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^6.0.3",
    "laravel-mix": "^5.0.1",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.1",
    "vue-template-compiler": "^2.6.11"
  }
}
